{
    "name": "donald/content-to-product",
    "description": "Content to product is a woocommerce plugin that allows owners of websites designed with the wordpress cms to sell the articles they publish.",
    "license": "GNU",
    "authors": [
        {
            "name": "57-house",
            "email": "njiegnamnjuhdonald@gmail.com"
        }
    ],
    "autoload": {
        "files" : [
            "includes/helpers.php"
        ],
        "psr-4": {
            "CTP\\" : "includes/"
        }
    },
    "require-dev": {
        "yoast/phpunit-polyfills": "^1.0",
        "yoast/wp-test-utils": "^1.0",
        "phpcompatibility/phpcompatibility-wp": "*",
        "wp-coding-standards/wpcs": "^2.3",
        "dealerdirect/phpcodesniffer-composer-installer": "^0.7",
        "phpunit/phpunit": "^7.4"
    },
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
